<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>First_Name_Not_Blank</fullName>
        <active>true</active>
        <errorConditionFormula>OR( ISBLANK( FirstName ) , ISNUMBER( FirstName ) )</errorConditionFormula>
        <errorDisplayField>FirstName</errorDisplayField>
        <errorMessage>Please Insert First Name and It must be in Character</errorMessage>
    </validationRules>
</CustomObject>
