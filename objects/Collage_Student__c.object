<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>test_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISBLANK( Name ),ISNUMBER( Name ))</errorConditionFormula>
